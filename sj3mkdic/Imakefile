#include "../../sj3.tmpl"

INCLUDES = -I../include -I../../include

INSTFLAGS       = -o $(SJ3OWNER) -g $(SJ3GROUP) -m 755

SRCS = \
	char.c \
	cnvhinsi.c \
	file.c \
	global.c \
	hindo.c \
	knjcvt.c \
	makedict.c \
	makelist.c \
	makeseg.c \
	memory.c \
	offset.c \
	readline.c \
	string.c

OBJS = \
	char.o \
	cnvhinsi.o \
	file.o \
	global.o \
	hindo.o \
	knjcvt.o \
	makedict.o \
	makelist.o \
	makeseg.o \
	memory.o \
	offset.o \
	readline.o \
	string.o

AllTarget(sj3mkdic)
NormalProgramTarget(sj3mkdic,$(OBJS),,,)
DependTarget()
InstallProgramWithFlags(sj3mkdic,$(SJ3BINDIR),$(INSTFLAGS))
