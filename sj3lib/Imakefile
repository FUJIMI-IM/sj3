#include "../sj3.tmpl"

#if SystemV4 || SystemV
DEFINES = $(USETLI)
#endif
INCLUDES = -I../include -I../sj3h
SRCS = \
	level1.c \
	sj.c \
	string.c 

OBJS = \
	level1.o \
	sj.o \
	string.o 

NormalLibraryObjectRule()
NormalLibraryTarget(sj3lib,$(OBJS))
DependTarget()
InstallLibrary(sj3lib,$(SJ3LIBDIR))
InstallNonExecFile(sj3lib.h,$(SJ3HDRDIR))
